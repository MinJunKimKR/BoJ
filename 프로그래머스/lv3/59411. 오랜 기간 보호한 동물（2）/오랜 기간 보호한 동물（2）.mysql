SELECT o.ANIMAL_ID, o.NAME FROM ANIMAL_OUTS o 
JOIN ANIMAL_INS i 
ON o.ANIMAL_ID = i.ANIMAL_ID
ORDER BY (o.DATETIME-i.DATETIME) desc
limit 2;


















